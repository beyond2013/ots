<p id="notice"><%= notice %></p>

<p>
  <strong>Program sessions:</strong>
  <%= @paper.program_session.program_with_session %>
</p>

<p>
  <strong>Course instructors:</strong>
  <%= @paper.course_instructor.course_with_instructor %>
</p>

<p>
  <strong>Exam:</strong>
  <%= @paper.exam.title %>
</p>

<p>
  <strong>Timeallowed:</strong>
  <%= @paper.timeallowed %>
</p>
<% @paper.questions.each do |question| %>
<b>Question: </b>
<%=h question.description %>
<b>Marks: </b><%= question.marks %>
<br></br>
<b>Choices: </b>
<ul>
<% question.choices.each do |choice| %>
  <li><%=h choice.description %></li>
<% end %>
<br></br>
</ul>
<% end %>

<%= link_to 'Edit', edit_paper_path(@paper) %> |
<%= link_to 'Back', papers_path %>
