<p id="notice"><%= notice %></p>

<h1>Papers</h1>

<table>
  <thead>
    <tr>
      <th>Program sessions</th>
      <th>Course instructors</th>
      <th>Exam</th>
      <th>Timeallowed</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @papers.each do |paper| %>
      <tr>
        <td><%= paper.program_session.program.name_with_nature %></td>
        <td><%= paper.course_instructor.course_with_instructor %></td>
        <td><%= paper.exam.title %></td>
        <td><%= paper.timeallowed %></td>
        <td><%= link_to 'Show', paper %></td>
        <td><%= link_to 'Edit', edit_paper_path(paper) %></td>
        <td><%= link_to 'Destroy', paper, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Paper', new_paper_path %>
