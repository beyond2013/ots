<%= form_with(model: program_session, local: true) do |form| %>
  <% if program_session.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(program_session.errors.count, "error") %> prohibited this program_session from being saved:</h2>

      <ul>
      <% program_session.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :program_id %>
    <%= form.collection_select :program_id, Program.all, :id, :name_with_nature %>
  </div>

  <div class="field">
    <%= form.label :session_id %>
    <%= form.collection_select :session_id, Session.all, :id, :session_start_finish %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
